{
  "name": "hilsager/yii2-api-paykeeper",
  "version":"0.0.2",
  "description": "Yii2 модуль для связи с API PayKeeper",
  "keywords": ["paykeeper"],
  "type": "project",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2-httpclient": "~2.0.0",
    "yiisoft/yii2": "*"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-jui": "*",
    "yiisoft/yii2-faker": "*"
  },
  "autoload": {
    "psr-4": {
      "hilsager\\components\\PayKeeperApi\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "process-timeout": 1800
  }
}
